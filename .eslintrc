{
  "ecmaFeatures": {
    "globalReturn": true,
    "jsx": true,
    "modules": true
  },

  "env": {
    "browser": false,
    "es6": true,
    "node": true,
  },

  "globals": {
  },
  "extends": [],
  "parser": "babel-eslint",
  "rules": {
    "prefer-arrow-callback": [1],
    "space-before-function-paren": [2, "never"],
    "no-param-reassign": [2, {"props": true}],
    "brace-style": [2, "1tbs", { "allowSingleLine": true }],
    "func-names": [0],
    "max-len": [0],
    "consistent-return": [0],
    "no-else-return": [0],
    "new-cap": [0],
    "arrow-body-style": [0],
    "react/no-danger": [0],
    "no-script-url": [0],
  }
}
